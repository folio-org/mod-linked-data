{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Metadata of the instance resource",
  "properties": {
    "source": {
      "type": "string",
      "enum": ["MARC", "LINKED_DATA"],
      "description": "The source of the instance"
    },
    "inventoryId": {
      "type": "string",
      "description": "ID of the instance in the Inventory application"
    },
    "srsId": {
      "type": "string",
      "description": "ID of the MARC bib record in the SRS application"
    }
  }
}
