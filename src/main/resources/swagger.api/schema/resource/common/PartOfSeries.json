{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Schema for a 'part of series' object which indicates if a work is part of a series.",
  "allOf": [
    {
      "type": "object",
      "properties": {
        "name": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/lite/name"
        },
        "issn": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/library/issn"
        },
        "volume": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/library/volume"
        }
      }
    }
  ]
}
