{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Instance request DTO",
  "allOf": [
    {
      "$ref": "../common/ProfileIdField.json"
    },
    {
      "$ref": "TitleFieldRequest.json"
    },
    {
      "type": "object",
      "properties": {
        "production": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "ProviderEventRequest.json"
          },
          "x-json-property": "http://bibfra.me/vocab/library/production"
        },
        "publication": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "ProviderEventRequest.json"
          },
          "x-json-property": "http://bibfra.me/vocab/library/publication"
        },
        "distribution": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "ProviderEventRequest.json"
          },
          "x-json-property": "http://bibfra.me/vocab/library/distribution"
        },
        "manufacture": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "ProviderEventRequest.json"
          },
          "x-json-property": "http://bibfra.me/vocab/library/manufacture"
        },
        "map": {
          "type": "array",
          "items": {
            "title": "IdentifierField",
            "oneOf": [
              {
                "type": "object",
                "title": "LccnField",
                "properties": {
                  "Lccn": {
                    "type": "object",
                    "$ref": "IdentifierRequest.json",
                    "x-json-property": "http://library.link/identifier/LCCN",
                    "x-field-extra-annotation": "@org.folio.linked.data.validation.LccnPatternConstraint"
                  }
                }
              },
              {
                "type": "object",
                "title": "IssnField",
                "properties": {
                  "Issn": {
                    "type": "object",
                    "$ref": "IdentifierRequest.json",
                    "x-json-property": "http://library.link/identifier/ISSN"
                  }
                }
              },
              {
                "type": "object",
                "title": "IsbnField",
                "properties": {
                  "Isbn": {
                    "type": "object",
                    "$ref": "IdentifierRequest.json",
                    "x-json-property": "http://library.link/identifier/ISBN"
                  }
                }
              },
              {
                "type": "object",
                "title": "IanField",
                "properties": {
                  "Ian": {
                    "type": "object",
                    "$ref": "IdentifierRequest.json",
                    "x-json-property": "http://library.link/identifier/IAN"
                  }
                }
              },
              {
                "type": "object",
                "title": "OtherIdField",
                "properties": {
                  "Identifier": {
                    "type": "object",
                    "$ref": "IdentifierRequest.json",
                    "x-json-property": "http://library.link/identifier/UNKNOWN"
                  }
                }
              }
            ]
          },
          "x-json-property": "http://library.link/vocab/map"
        },
        "supplementaryContent": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "../common/SupplementaryContent.json"
          },
          "x-json-property": "http://bibfra.me/vocab/library/supplementaryContent"
        },
        "accessLocation": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "../common/AccessLocation.json"
          },
          "x-json-property": "http://bibfra.me/vocab/library/accessLocation"
        },
        "media": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "../common/Category.json"
          },
          "x-json-property": "http://bibfra.me/vocab/library/media"
        },
        "carrier": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "../common/Category.json"
          },
          "x-json-property": "http://bibfra.me/vocab/library/carrier"
        },
        "edition": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/library/edition"
        },
        "copyright": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "../common/CopyrightEvent.json"
          },
          "x-json-property": "http://bibfra.me/vocab/library/copyright"
        },
        "extent": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "../common/Extent.json"
          },
          "x-json-property": "http://bibfra.me/vocab/lite/extent"
        },
        "dimensions": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/library/dimensions"
        },
        "projectProvisionDate": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/bflc/projectedProvisionDate",
          "description": "Projected publication date (YYMM)"
        },
        "issuance": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/library/issuance"
        },
        "statementOfResponsibility": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/library/statementOfResponsibility"
        },
        "bookFormat": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "../common/Category.json"
          },
          "x-json-property": "http://bibfra.me/vocab/library/bookFormat"
        },
        "publicationFrequency": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "../common/PublicationFrequency.json"
          },
          "x-json-property": "http://bibfra.me/vocab/library/publicationFrequency"
        },
        "adminMetadata": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "../common/AdminMetadata.json"
          },
          "x-json-property": "http://bibfra.me/vocab/library/adminMetadata"
        },
        "_notes": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "../common/Note.json"
          },
          "x-json-property": "_notes"
        },
        "_workReference": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "../common/IdField.json"
          }
        }
      }
    }
  ]
}
