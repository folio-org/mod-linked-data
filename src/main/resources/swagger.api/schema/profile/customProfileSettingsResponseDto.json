{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Workspace custom profile settings response DTO",
  "allOf": [
    {
      "$ref": "customProfileSettings.json"
    },
    {
      "type": "object",
      "properties": {
        "profileId": {
          "type": "integer",
          "format": "int16",
